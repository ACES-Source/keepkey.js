{
  "name": "@keepkey/core",
  "version": "2.0.0",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "source": "src/index.ts",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.m.js",
  "scripts": {
    "build": "microbundle --name=keepkeyCore",
    "build:browser": "microbundle --target=web --external=none --name=keepkeyCore",
