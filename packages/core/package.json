    "dev": "microbundle watch --name=keepkeyCore",
    "dev:browser": "microbundle watch --target=web --external=none --name=keepkeyCore",
    "prepublishOnly": "yarn clean && yarn build"
  },
  "dependencies": {
    "@keepkey/device-protocol": "^6.0.2",
    "@types/eventemitter2": "^4.1.0",
    "bytebuffer": "^5.0.1",
    "eventemitter2": "^5.0.1",
    "google-protobuf": "^3.7.0-rc.2",
    "rx-singleton-lock": "^1.1.1",
    "rxjs": "^6.4.0"
  },
  "devDependencies": {
    "@types/bytebuffer": "^5.0.40",
    "@types/google-protobuf": "^3.2.7",
    "microbundle": "^0.9.0"
  }
}
