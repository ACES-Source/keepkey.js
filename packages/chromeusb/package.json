{
  "name": "@keepkey/chromeusb",
  "version": "2.0.0",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "source": "src/index.ts",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.m.js",
  "scripts": {
    "build": "microbundle --target=web --name=keepkeyChromeUSB",
    "clean": "rm -rf dist .rts2_*",
    "dev": "microbundle watch --target=web --name=keepkeyChromeUSB --compress=false",
    "prepublishOnly": "yarn clean && yarn build"
  },
  "dependencies": {
    "@keepkey/core": "^2.0.0",
    "@keepkey/keepkey.js": "^2.0.0",
    "bytebuffer": "^5.0.1",
    "eventemitter3": "^3.1.0"
  },
  "devDependencies": {
    "@types/bytebuffer": "^5.0.40",
    "@types/chrome": "^0.0.81",
    "@types/eventemitter3": "^2.0.2",
    "@types/w3c-web-usb": "^1.0.1",
    "microbundle": "^0.9.0"
  }
}
