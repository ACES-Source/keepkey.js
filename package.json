{
  "name": "keepkey.js",
  "version": "2.0.0",
  "main": "index.js",
  "private": true,
  "repository": "https://github.com/keepkey/keepkey.js",
  "author": "spiceboi <trappist.spice@gmail.com>",
  "license": "MIT",
  "workspaces": {
    "packages": [
      "packages/*",
      "examples/*"
    ],
    "nohoist": [
      "**/@types/w3c-web-usb",
      "**/@types/w3c-web-usb/**"
    ]
  },
  "scripts": {
    "build:webusb": "lerna run build --scope @keepkey/webusb --include-filtered-dependencies",
    "dev:webusb": "lerna run dev --scope webusb --parallel --include-filtered-dependencies"
  },
  "devDependencies": {
    "lerna": "^3.13.0",
    "standard": "^12.0.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.3",
    "typescript-eslint-parser": "^22.0.0"
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "plugins": [
      "typescript"
    ]
  }
}
